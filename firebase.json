{
  "hosting": {
    "public": "out",
    "cleanUrls": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/blog",
        "destination": "/articles",
        "type": 301
      },
      {
        "source": "/articles/on_iteration_and_lean_thinking",
        "destination": "/articles/a_case_for_procrastination",
        "type": 301
      }
    ]
  }
}
